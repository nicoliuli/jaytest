<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.jaytest.dao.JiMapper">

    <select id="JigetDemo" parameterType="Integer" resultType="com.jaytest.model.Ji">
        select * from ti where id=#{id} limit 1
    </select>

    <select id="queryDemo" parameterType="Integer" resultType="com.jaytest.model.Ji">
        select id,pid,stime from ti where id=#{id}
    </select>

</mapper>